{% if permission.groupPermission.edit %}
    {% form_theme group 'bootstrap_3_horizontal_layout.html.twig'  %}

    <div class="widget_EditGroup widgetBoxShadow">

        <div class="formBlock positionRelative" data-divForm="1">
            {{form_start(group)}}
            {{ form_widget(group)}}
            {{ form_end(group) }}
        </div>
        <!-- // Options END -->
    </div>
{% endif %}
{%if fetch_widget_EditBanner is defined %}
    {{ fetch_widget_EditBanner|raw  }}
{%endif%}
<div class="button-action">
    <a href="javascript:history.back()"> 
        <div class="btnGoBack" id="btnGoBack">
            <i class="fa fa-arrow-circle-o-left"></i>
        </div>
    </a>
    <div class="buttonDefault  buttonGreen" data-submit="1">
        <a>
            <div type="submit"  class="add">Salva Permessi</div>
            <i class=" fa  fa-save"></i>
        </a>
    </div>

    <div class="buttonDefault buttonRed" data-reset="1">
        <a>
            <div type="submit"  class="add"> Reset Permessi</div>
            <i class=" fa   fa-eraser"></i>
        </a>
    </div>
    <div id="toTop">
        <i class="fa fa-arrow-circle-o-left"></i>
    </div>

</div>
