{% form_theme tecnical 'bootstrap_3_horizontal_layout.html.twig'  %}

<div class="widget_EditTecnicalTemplate widgetBoxShadow">

    <div class="formBlock positionRelative" data-divForm="1">
        {{form_start(tecnical)}}
        {{ form_widget(tecnical)}}
        {{ form_end(tecnical) }}
    </div>
    <!-- // Options END -->
</div>
{%if fetch_widget_EditTecnicalTemplate is defined %}
    {{ fetch_widget_EditTecnicalTemplate|raw  }}
{%endif%}
<div class="button-action">
    <a href="javascript:history.back()"> 
        <div class="btnGoBack" id="btnGoBack">
            <i class="fa fa-arrow-circle-o-left"></i>
        </div>
    </a>
    <div class="buttonDefault  buttonGreen" data-submit="1">
        <a>
            <div type="submit"  class="add">Salva Marchio</div>
            <i class=" fa  fa-save"></i>
        </a>
    </div>

    <div class="buttonDefault buttonRed" data-reset="1">
        <a>
            <div type="submit"  class="add"> Reset Marchio</div>
            <i class=" fa   fa-eraser"></i>
        </a>
    </div>
    <div class="buttonDefault" onclick="window.location.href = '{{ path('tecnicalTemplate') }}';">
        <div class="add"> Lista Temnplate Schdede Tecniche</div>
        <i class=" fa    fa-arrow-circle-left"></i>
    </div>
    <div id="toTop">
        <i class="fa fa-arrow-circle-o-left"></i>
    </div>

</div>
