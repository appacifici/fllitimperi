{% form_theme redirect 'bootstrap_3_horizontal_layout.html.twig'  %}

<div class="widget_EditRedirect widgetBoxShadow">
      <div class="formBlock positionRelative" data-divForm="1">
        {{form_start(redirect)}}
        {{ form_widget(redirect)}}
        {{ form_end(redirect) }}
    </div>
</div>
        
{%if fetch_widget_EditRedirect is defined %}
    {{ fetch_widget_EditRedirect|raw  }}
{%endif%}

<div class="button-action">
    <a href="javascript:history.back()"> 
        <div class="btnGoBack" id="btnGoBack">
            <i class="fa fa-arrow-circle-o-left"></i>
        </div>
    </a>
    <div class="buttonDefault  buttonGreen" data-submit="1">
        <a>
            <div type="submit"  class="add">Salva Redirect</div>
            <i class=" fa  fa-save"></i>
        </a>
    </div>
    <div class="buttonDefault buttonRed" data-reset="1">
        <a>
            <div type="submit"  class="add"> Reset Redirect</div>
            <i class=" fa   fa-eraser"></i>
        </a>
    </div>
    <div id="toTop">
        <i class="fa fa-arrow-circle-o-left"></i>
    </div>

</div>
