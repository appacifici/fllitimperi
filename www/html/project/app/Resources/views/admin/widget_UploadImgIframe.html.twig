<div class="widget_UploadImgIframe widgetBoxShadow">
     <div class="formBlock">
        <form id="myFormUploadImg" class="positionRelative" method="post" action="/admin/uploadImgArticleIframe" target="myIframe" enctype="multipart/form-data">
            <div class="form-group">
                <label class="col-sm-2 control-label" for="form_position">Immagine</label>
                    <div class="col-sm-10">
                        <input type="file" name="src">
                    </div> 
                <div class="clearB"></div>
            </div>
            <div class="form-group">
                <label class="col-sm-2 control-label" for="form_position">Titolo Immagine</label>
                    <div class="col-sm-10">
                        <input class="form-control" type= "text" name="title">
                    </div> 
                <div class="clearB"></div>
            </div>
            <input type="submit" class="pull-right buttonDefault buttonGreen" value="Carica Immagini" style="width: 100%; margin-top: 10px; color: #fff; margin: 0 auto;">
              
        </form>
        <iframe class="iFrameUpldImg" name="myIframe"></iframe>
    </div>
</div>
<div class="button-action">
    <a href="javascript:history.back()"> 
        <div class="btnGoBack" id="btnGoBack">
            <i class="fa fa-arrow-circle-o-left"></i>
        </div>
    </a>
    <div class="toTop" id="toTop">
        <i class="fa fa-arrow-circle-o-left"></i>
    </div>

</div>
{%if fetch_widget_UploadImgIframe is defined %}
    {{ fetch_widget_UploadImgIframe|raw  }}
{%endif%}
