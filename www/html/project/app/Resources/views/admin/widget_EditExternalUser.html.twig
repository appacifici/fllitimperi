{% if permission.externalUser.edit %}
    {% form_theme user 'bootstrap_3_horizontal_layout.html.twig'  %}

    <div class="widget_EditUser widgetBoxShadow">
          <div class="formBlock positionRelative" data-divForm="1">
            {{form_start(user)}}
            {{ form_widget(user)}}
            {{ form_end(user) }}
        </div>
        <!-- // Options END -->
    </div>
{% endif %}
{%if fetch_widget_EditExternalUser is defined %}
    {{ fetch_widget_EditExternalUser|raw  }}
{%endif%}

<div class="button-action">
    <a href="javascript:history.back()"> 
        <div class="btnGoBack" id="btnGoBack">
            <i class="fa fa-arrow-circle-o-left"></i>
        </div>
    </a>
    <div class="buttonDefault  buttonGreen" data-submit="1">
        <a>
            <div type="submit"  class="add">Salva Utente</div>
            <i class=" fa  fa-save"></i>
        </a>
    </div>
    <div class="buttonDefault buttonRed" data-reset="1">
        <a>
            <div type="submit"  class="add"> Reset Utente</div>
            <i class=" fa   fa-eraser"></i>
        </a>
    </div>
    <div id="toTop">
        <i class="fa fa-arrow-circle-o-left"></i>
    </div>

</div>
